{
  "name": "@nativescript-copilot/vue",
  "version": "0.2.2",
  "description": "Nativescript-Vue App Tour & Showcase",
  "main": "dist/nativescript-copilot.umd.js",
  "module": "dist/nativescript-copilot.esm.js",
  "unpkg": "dist/nativescript-copilot.js",
  "files": [
    "dist/*",
    "src/*",
    "types/*",
    "LICENSE"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "echo \"Error: no test specified\"",
    "clean": "rimraf dist/*",
    "preversion": "npm run clean && npm test",
    "version": "npm run build && git add -A",
    "postversion": "git push --tags origin master",
    "prep:patch": "npm version patch -m \"build %s\"",
    "prep:minor": "npm version minor -m \"build %s\"",
    "prep:major": "npm version major -m \"build %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jawa-the-hutt/nativescript-vue-copilot.git"
  },
  "keywords": [
    "vue",
    "nativescript",
    "typescript",
    "tour",
    "showcase",
    "walkthrough",
    "coachmarks",
    "instructions",
    "guide",
    "intro"
  ],
  "author": "Gary Gambill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jawa-the-hutt/nativescript-copilot/issues"
  },
  "homepage": "https://github.com/jawa-the-hutt/nativescript-copilot#readme",
  "dependencies": {
    "nativescript-tweenjs": "^0.9.0",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "minimist": "^1.2.0",
    "nativescript-vue": "^2.4.0",
    "nativescript-vue-template-compiler": "^2.4.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.24.2",
    "rollup-plugin-vue": "^6.0.0",
    "tns-core-modules": "^6.1.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.4.3",
    "vue": "^2.6.10",
    "vue-eslint-parser": "^6.0.4",
    "vue-template-compiler": "^2.6.10"
  }
}
